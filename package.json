  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastianstupak/toshi-view.git"
  },
  "author": "@sebastianstupak",
  "license": "MIT",
